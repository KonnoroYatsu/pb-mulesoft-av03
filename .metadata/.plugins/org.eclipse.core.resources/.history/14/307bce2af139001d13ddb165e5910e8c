package com.avaliacao03.avaliacao03.controller.dto;

public class UsuariosDto {
	private long id;
	private String nome;
	private String email;
	private String senha;
	
	public UsuariosDto() {}

	public UsuariosDto(long id, String nome, String email, String senha) {
		this.id = id;
		this.nome = nome;
		this.email = email;
		this.senha = senha;
	}
	
	
}
